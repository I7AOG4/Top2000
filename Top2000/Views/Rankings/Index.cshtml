@model IEnumerable<Top2000.EF.tblRanking>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tblSongs.SongName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RankingPosition)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RankingYear)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tblSongs.SongName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RankingPosition)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RankingYear)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RankingID }) |
            @Html.ActionLink("Details", "Details", new { id=item.RankingID }) |
